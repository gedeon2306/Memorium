<%-include("./composants/header") %>
    <section class="sectionTrou">
        <% const N = 250; for (let i = 1; i <= N; i++) { %>
            <% let isRed = false; %>
            <% resultat.forEach(element => { %>
                <% if (element.place === i) { %>
                    <% isRed = true; id = element.id %>
                <% } %>
            <% }); %>

            <% if (isRed) { %>
                <div class="cartTrou" style="background-color: #ff0000;"><a href="/defunt.show/<%=id %>"><%= i %></a></div>
            <% } else { %>
                <div class="cartTrou"><a><%= i %></a></div>
            <% } %>
        <% } %>
    </section>        
<%-include("./composants/footer") %>