<% if (locals.flash && locals.flash.error) { %>
    <script>
        Swal.fire({
            icon: "error",
            title: "Erreur",
            text: "<%=flash.error%>",
        });
    </script>
<% } %>

<% if (locals.flash && locals.flash.success) { %>
    <script>
        Swal.fire({
            icon: "success",
            title: "Success",
            text: "<%=flash.success%>",
        });
    </script>
<% } %>